{
  "name": "simple-audio-web-player",
  "version": "0.1.0",
  "description": "simple audio player for your website or application",
  "main": "./lib/player.node.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./dev.webpack.config.ts",
    "build:browser": "webpack --config ./build.webpack.config.ts --output-library-target global --output-filename player:browser.js",
    "build:node": "webpack --config ./build.webpack.config.ts --output-library-target commonjs --output-filename player:node.js"
  },
