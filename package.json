{
  "name": "simple-audio-web-player",
  "version": "0.1.0",
  "description": "simple audio player for your website or application",
  "main": "./lib/player.node.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./dev.webpack.config.js",
    "build-browser": "webpack --config ./build.webpack.config.js --output-library-target global --output-filename player.browser.js",
